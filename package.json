{
  "name": "ui-interview-evaluation-questions",
  "version": "1.0.0",
  "description": "Interview Questions for Evaluations",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "npx prettier . --write",
    "format:check": "npx prettier . --check",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "app:gen": "tsx --env-file=.env scripts/generate-new-app.ts",
    "postapp:gen": "npm run app:update",
    "app:update": "tsx --env-file=.env scripts/update-apps.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sujeet-pro/ui-interview-evaluation-questions.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sujeet-pro/ui-interview-evaluation-questions/issues"
  },
  "homepage": "https://github.com/sujeet-pro/ui-interview-evaluation-questions#readme",
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.8",
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.0.6",
    "daisyui": "^5.0.0-beta.8",
    "ejs": "^3.1.10",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.2",
    "tailwindcss": "^4.0.8",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
  }
}
